{
  "name": "reactrs",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://qoolebyaqa.github.io/ReactRS/",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:fix": "prettier . --write . --ext ts,tsx",
